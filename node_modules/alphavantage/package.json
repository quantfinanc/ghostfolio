{
  "name": "alphavantage",
  "version": "2.2.0",
  "description": "A simple interface to the Alpha Vantage API.",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "jest --setupFiles dotenv/config",
    "lint": "prettier --write \"./!(dist)**/**.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "rollup index.js --file dist/bundle.js --format umd --name alphavantage"
  },
  "keywords": [
    "alpha vantage",
    "vantage",
    "alpha",
    "alphavantage",
    "stocks",
    "stock market",
    "api"
  ],
  "author": "Zack Urben <zackurben@gmail.com> (http://zackurben.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackurben/alphavantage"
  },
  "homepage": "https://github.com/zackurben/alphavantage",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-jest": "^26.1.0",
    "coveralls": "^3.1.0",
    "delay": "^4.4.0",
    "dotenv": "^8.2.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0"
  },
  "jest": {
    "collectCoverage": true,
    "bail": false,
    "verbose": true,
    "testMatch": [
      "**/test/**.js"
    ],
    "testEnvironment": "node"
  }
}
